{
  "compilerOptions": {
    "target" : "ES2015",
    "module": "system",
    "noImplicitAny": true,
    "removeComments": true,
    "forceConsistentCasingInFileNames": true,
    "preserveConstEnums": true,
    "outFile": "../../built/local/tsc.js",
    "sourceMap": true,
    "lib": ["es2015", "DOM", "ES6", "DOM.Iterable", "ScriptHost", "ES2016.Array.Include"]
  },
  "include": ["./**/*.ts"],
  "exclude": ["node_modules", "**/*.spec.ts"]
}
