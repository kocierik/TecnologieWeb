{
  "name": "game",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --host",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 5050",
    "typecheck": "vue-tsc --noEmit",
    "format": "eslint --fix & prettier --write './**/*.{js,jsx,ts,tsx,css,md,json,vue}' --config ./.prettierrc",
    "lint": "eslint --ext  src/**/*.vue,src/**/*.js,src/**/*.jsx,src/**/*.cjs,src/**/*.mjs,src/**/*.ts,src/**/*.tsx,src/**/*.cts,src/**/*.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "party-js": "^2.1.2",
    "random-animals-api": "^1.2.6",
    "shared": "file:../shared",
    "sweetalert": "^2.1.2",
    "vue": "^3.2.33",
    "vue-router": "^4.0.14"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/node": "^16.11.33",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.5",
    "eslint": "^8.14.0",
    "eslint-plugin-vue": "^8.7.1",
    "postcss": "^8.4.12",
    "prettier": "^2.5.1",
    "sass": "^1.51.0",
    "sass-loader": "^12.6.0",
    "tailwindcss": "^3.0.24",
    "typescript": "~4.6.3",
    "vite": "^2.9.5",
    "vue-tsc": "^0.34.7"
  }
}
